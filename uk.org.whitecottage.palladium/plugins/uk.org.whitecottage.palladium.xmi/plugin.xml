<?xml version="1.0" encoding="UTF-8"?>
<plugin>

   <extension
         id="uk.org.whitecottage.palladium.xmi.fragment"
         point="org.eclipse.e4.workbench.model">
      <fragment
            uri="fragment.e4xmi"
            apply="always">
      </fragment>
   </extension>
   <extension
         id="popup:org.eclipse.papyrus.views.modelexplorer.modelexplorer.popup?after=diagramedit"
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.papyrus.views.modelexplorer.modelexplorer.popup?after=diagramedit">
         <command
               commandId="uk.org.whitecottage.palladium.xmi.command.import"
               label="Import XMI..."
               mode="FORCE_TEXT"
               style="push">
         </command>
      </menuContribution>
   </extension>

</plugin>
